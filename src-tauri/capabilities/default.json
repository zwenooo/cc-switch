{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "updater:default",
    "core:window:allow-set-skip-taskbar",
    "process:allow-restart",
    "dialog:default"
  ]
}
